{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "types": ["vite/client"],
    "paths": {
      "@domain/*": ["domain/*"],
      "@gl/*": ["gl/*"]
    }
  },
  "include": ["src"]
}
